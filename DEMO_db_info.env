# This should be copied and renamed to db_info.env

# The database credentials set here will apply as the database is imported into the mysql container - DO NOT UPLOAD THE .ENV FILE TO GIT
db_name="example_database"
db_user="root"
db_passwd="password"
# The container name is defined in the docker-compose file
db_container="mysql"
# Any exising branch of infrastructure-portal will work
infra_branch="prod"
# The databade port is defined in the docker-compose file
db_port="3306"
